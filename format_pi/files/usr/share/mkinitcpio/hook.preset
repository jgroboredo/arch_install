ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-%PKGBASE%"

PRESETS=('REPLACE_ME_PRESET')

# BIOS
bios_image="/boot/initramfs-%PKGBASE%.img"

# EFI
efi_image="/boot/initramfs-%PKGBASE%.img"
#efi_options="--splash /usr/share/systemd/bootctl/splash-arch.bmp"
efi_microcode=(/boot/*-ucode.img)
efi_efi_image="/efi/EFI/Linux/%PKGBASE%.efi"
